   <chart type="geo" width="800" height="600"   align="left" id="chart4_div">
       <title>World Wide</title>
  
       <columns>
            <column name="Country" type="string"/>
            <column name="Popularity"  type="number"/>     
       </columns>
  
       <rows>
            <row name="Germany" value="200" />
            <row name="United States" value="300" /> 
            <row name="Brazil" value="400" />      
            <row name="Canada" value="500" />
            <row name="France" value="600" />
            <row name="RU" value="700" />        
       </rows>
   </chart>
  
   <chart type="gauge" width="600" height="300"   align="left" id="chart5_div">
       <title>Resource Utilization</title>
  
       <columns>
            <column name="Label" type="string"/>
            <column name="Value"  type="number"/>     
       </columns>
  
       <rows>
            <row name="Memory" value="80" />
            <row name="CPU" value="55" /> 
            <row name="Network" value="68" />               
