            <row name1="GBR" value1="80.05" value2="2" name2="Europe" value3="61801570"/>
            <row name1="IRN" value1="72.49" value2="1.7" name2="Middle East" value3="73137148"/>
            <row name1="IRQ" value1="68.09" value2="4.77" name2="Middle East" value3="31090763"/>
