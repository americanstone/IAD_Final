<!DOCTYPE charts SYSTEM "charts.dtd">
<charts num_charts="5">

 
   <chart type="pie" width="800" height="600"   align="left" id="chart1_div">
 
       <title>How Much Pizza I Ate Last Night</title>
  
       <columns>
            <column name="Topping" type="string"/>
            <column name="Slices"  type="number"/>     
       </columns>
  
       <rows>
            <row name="Mushrooms" value="3" />
            <row name="Onions" value="1" /> 
            <row name="Olives" value="1" />      
            <row name="Zucchini" value="1" />
            <row name="Pepperoni" value="2" />   
       </rows>
   </chart>
  
   <chart type="bar" width="800" height="600"   align="left" id="chart2_div">
       <title>Company Performance</title>
  
       <columns>
            <column name="Year" type="string"/>
            <column name="Sales"  type="number"/>    
            <column name="Expenses"  type="number"/> 
       </columns>
  
       <rows>
            <row name="2008" value1="1000" value2="300"/>
            <row name="2009" value1="1200" value2="650"/>
            <row name="2010" value1="900" value2="230"/>
 
       </rows>
   </chart>
  
